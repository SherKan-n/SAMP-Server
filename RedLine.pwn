	{
	    case 0: 
		{
			KillTimer_(ScriptTimer[15]); 
			SetTimer_("GameModeInitExitFunc", 1000, 1000, 1);
		}
	    case 1: GameTextForAll("~w~Restart server ~b~NOW!", 1510, 5), restartsv = 0;
	    case 2: GameTextForAll("~w~Restart server in ~r~2~w~ secunde", 1510, 5);
