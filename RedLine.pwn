	mysql_escape_string(carname, escstr);
	format(query, sizeof(query), "SELECT `ID` FROM `stock` WHERE `Name` = '%s' LIMIT 1", escstr);
	new Cache:car = mysql_query(SQL, query);
	if(cache_num_rows() == 0)
	{
	    cache_delete(car);
		return 0;
	}
	else
	{
		new strid[32], intid;
		cache_get_value_index(0, 0, strid);
		intid = strval(strid);
	    cache_delete(car);
		return intid;
	}
}
