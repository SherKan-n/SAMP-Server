stock FindIP(const string[])
{
    new dotCount;
    for(new i; string[i] != EOS; i++)
    {
        if(('0' <= string[i] <= '9') || string[i] == '.' || string[i] == ':')
        {
            if((string[i] == '.') && (string[i + 1] != '.') && ('0' <= string[i - 1] <= '9')) dotCount++;
            continue;
        }
    }
    return (dotCount > 2);
}
