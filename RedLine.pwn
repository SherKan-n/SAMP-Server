		format(string, sizeof(string), "{52CC52}%s donated %s$ for players with level of between 2-7.", sendername,FormatNumber(moneycharity));
		SendFactionMessage(7, COLOR_WHITE, string);
		FactionInfo[7][fSafeMoney] += moneycharity;
		SaveFactions(7);
