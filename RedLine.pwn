			GivePlayerWeaponEx(playerid, 24, 100);
			GivePlayerWeaponEx(playerid, 30, 200);
			GivePlayerWeaponEx(playerid, 26, 30);
			GivePlayerWeaponEx(playerid, 39, 5);
			GivePlayerWeaponEx(playerid, 4, 1);
