				GivePlayerWeaponEx(playerid, 23, 200);
				GivePlayerWeaponEx(playerid, 34, 40);
				GivePlayerWeaponEx(playerid, 4, 1);
				GivePlayerWeaponEx(playerid, 29, 150);
