			{
				format(string, sizeof(string), "{B8FFDB}** %s are nevoie de un medic, Locatie: %s (SF). Foloseste [/accept medic %d] pentru a accepta apelul.", sendername,zone,playerid);
				SendFactionMessage(4, COLOR_WHITE, string);
			}
